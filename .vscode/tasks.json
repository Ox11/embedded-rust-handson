{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"options": {
				"cwd": "${workspaceFolder}/my_nucleo_rust_sensor"
			},
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"env": {
                "DEFMT_LOG": "warn"
			},
			"label": "rust: cargo build"
		}
	]
}